{
  "presets": [
    "react",
    "stage-0",
    "es2015"
  ],
  "comments": true,
  "plugins": [
    "transform-runtime",
    "react-hot-loader/babel",
    "transform-decorators-legacy",
    "dva-hmr",
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": true
      }
    ],
    [
      "transform-class-properties",
      {
        "spec": true
      }
    ]
  ],
  "env": {
    "development": {
      "plugins": [
        [
          "react-transform",
          {
            "transforms": [
              {
                "transform": "react-transform-catch-errors",
                "imports": ["react", "redbox-react"]
              }
            ]
          }
        ]
      ]
    }
  }
}