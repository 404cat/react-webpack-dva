{
  "presets": [
    "react",
    "stage-0",
    "es2015"
  ],
  "plugins": [
    "transform-runtime",
    "transform-decorators-legacy",
    [
      "import",
      {
        "libraryName": "antd"
      }
    ],
    [
      "transform-class-properties",
      {
        "spec": true
      }
    ]
  ]
}